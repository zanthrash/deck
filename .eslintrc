{
  "ecmaFeatures": {
    "jsx": true,
  },
  "rules": {
    "semi": 2,
    "quotes": 0,
    "comma-dangle": 0,
    "no-mixed-requires": 0,
    "no-extra-boolean-cast": 0,
    "no-unused-vars": 0,
    "no-debugger": 0,
    "no-loop-func": 0,
    "space-infix-ops": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "no-spaced-func": 0,
    "no-alert": 0,
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
    "no-shadow": 0,
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jasmine": true,
  },
  "globals": {
    "angular": true,
    "$": true,
    "_": true,
    "__GATE_HOST__": true,
    "__AUTH_ENDPOINT__": true,
    "__BAKERY_DETAIL_URL__": true,
    "__FEEDBACK_URL__": true,
    "__DEFAULT_TIME_ZONE__": true,
    "__NETFLIX_ENABLED__": true,
    "__CLOUDFOUNDRY_ENABLED__": true,
    "__GOOGLE_ENABLED__": true,
    "__AMAZON_ENABLED__": true,
    "__TITAN_ENABLED__": true,
    "__WHATS_NEW_GIST_ID__" : true,
    "__WHATS_NEW_FILE_NAME__": true,
    "__WHATS_NEW_ACCESS_TOKEN__" : true,
    "__DEFAULT_PROVIDERS__": true,
    "__FEATURE_REBAKE_ENABELED__": true,
    "__FEATURE_NETFLIX_MODE_ENABELED__": true,
    "__FEATURE_BLESK_ENABLED__": true,
    "__FEATURE_FAST_PROPERTIES_ENABLED__": true,
    "__FEATURE_VPC_MIGRATOR_ENABELED__" : true,
    "__FEATURE_CLUSTER_DIFF_ENABLED__": true,
    "__FEATRUE_PIPELINES_ENABLED__" : true,
  }
}
